   "id": "8cf34b15-78b5-4894-a40e-879c4640a6c9",
   "metadata": {},
   "outputs": [],
   "source": [
    "i = len(str)-1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "27bfd7d0-8c6c-4015-9f17-48bfb5c7c97f",
   "metadata": {},
   "outputs": [],
   "source": [
    "while (i >=0):\n",
    "    rev_str += str[i]\n",
    "    i -= 1"
   ]
  },
